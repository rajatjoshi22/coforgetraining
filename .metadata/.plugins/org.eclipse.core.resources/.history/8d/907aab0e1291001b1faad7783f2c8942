package com.coforge.thread;

public class MainDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
     Thread t=new ChildThread();
     t.start();
     for(int i=0;i<5;i++) {
 		System.out.println(i+" *5= "+(i*5));
 	
 	try {
 	Thread.sleep(2000);
 	}catch(InterruptedException e) {
         e.printStackTrace();		
 	}
 
 }
	}

}
